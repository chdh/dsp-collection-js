{
   "name": "dsp-collection",
   "version": "0.2.8",
   "description": "A collection of JavaScript modules for digital signal processing (written in TypeScript)",
   "keywords": [
      "DSP",
      "Digital Signal Processing",
      "DFT",
      "Goertzel",
      "FFT",
      "Fourier Transform",
      "Filter",
      "Spectral filtering",
      "Window functions",
      "Instantaneous frequency",
      "Pitch estimation",
      "Simple moving average (SMA)",
      "triangular moving average (TMA)"
   ],
   "license": "MIT",
   "homepage": "http://www.source-code.biz/dsp/js",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/chdh/dsp-collection-js.git"
   },
   "contributors": [
      {
         "name": "Christian d'Heureuse",
         "email": "chdh@inventec.ch",
         "url": "http://www.source-code.biz"
      }
   ],
   "scripts": {
      "clean": "node build.js clean",
      "build": "node build.js build",
      "lint": "eslint src/**"
   },
   "type": "module",
   "exports": {
      "./*": {
         "types": "./dist/*.d.ts",
         "default": "./dist/*.js"
      }
   },
   "files": [
      "dist/"
   ]
}
