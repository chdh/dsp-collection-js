{
   "compilerOptions": {
      "alwaysStrict": true,
      "baseUrl": "src",
      "lib": ["ES6"],
      "module": "ES6",
      "moduleResolution": "node",
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noUnusedParameters": true,
      "outDir": "tempBuild",
      "paths": {
         "dsp-collection/*": [
            "../../dist/*"
         ]
       },
      "removeComments": true,
      "rootDir": "src",
      "sourceMap": true,
      "strictFunctionTypes": true,
      "strictNullChecks": true,
      "target": "ES6"
   }
}
