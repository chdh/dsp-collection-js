{
   "compilerOptions": {
      "allowUnreachableCode": false,
      "alwaysStrict": true,
      "baseUrl": "src",
      "lib": ["ES6"],
      "module": "node16",
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "noImplicitOverride": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      // "noUnusedLocals": true,
      "noUnusedParameters": true,
      "outDir": "tempBuild",
      "removeComments": true,
      "rootDir": "src",
      "sourceMap": true,
      "strictFunctionTypes": true,
      "strictNullChecks": true,
      "target": "ES2017"
   }
}
